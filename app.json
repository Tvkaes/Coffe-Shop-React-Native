{
  "expo": {
    "name": "coffe-shop",
    "slug": "coffe-shop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF",
        "permissions":["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE","CAMERA_ROLL"]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "my first app with react native ",
    "githubUrl": "https://github.com/Tvkaes/Coffe-Shop-React-Native"
  }
}
